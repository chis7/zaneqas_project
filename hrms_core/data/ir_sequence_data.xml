<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_hrms_strategic_plan" model="ir.sequence">
            <field name="name">Strategic Plan</field>
            <field name="code">hrms.strategic.plan</field>
            <field name="prefix">SP/%(range_year)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_hrms_departmental_work_plan" model="ir.sequence">
            <field name="name">Departmental Work Plan</field>
            <field name="code">hrms.departmental.work.plan</field>
            <field name="prefix">DWP/%(range_year)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_hrms_work_plan_activity" model="ir.sequence">
            <field name="name">Departmental Work Plan</field>
            <field name="code">hrms.departmental.work.plan.activity</field>
            <field name="prefix">ACT/%(range_year)s/</field>
            <field name="padding">9</field>
<!--            <field name="number_next">1</field>-->
<!--            <field name="number_increment">1</field>-->
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequences for hrms.competence -->
        <record id="hrms_competence_seq" model="ir.sequence">
            <field name="name">Competence</field>
            <field name="code">hrms.competence.seq</field>
            <field name="prefix">COMPE/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
         <!-- Sequences for hrms.competency -->
        <record id="hrms_competency_seq" model="ir.sequence">
            <field name="name">Competency</field>
            <field name="code">hrms.competency.seq</field>
            <field name="prefix">COMPY/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="hrms_qualification_seq" model="ir.sequence">
            <field name="name">Qualification</field>
            <field name="code">hrms.qualification.seq</field>
            <field name="prefix">QUAL/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
